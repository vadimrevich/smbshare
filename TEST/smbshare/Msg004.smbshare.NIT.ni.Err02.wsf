<!-- File wsf -->
<!-- Msg004.smbshare.NIT.ni.Err02.wsf -->
<!--
 * Test if smbshare NonInteractive Files is Installed
 * (errorlevel 2 Text)
-->
<!-- ***************************** -->
<job id="ID1">
	<script language="JScript"></script>
	<script language="JScript">
	<![CDATA[
	var aMsg02, iTimeOut;
	aMsg02 = "";
	iTimeOut = 6000; // 6 Sec

	aMsg02 += "Error! Ошибка!\n\n";
	aMsg02 += "Отсутствуют важные файлы установщика smbshare NonInteractive files.\n";
	aMsg02 += "Попробуйте переустановить пакет smbshrare и все его компоненты.\n";
	aMsg02 += "\nВ случае, если ошибка повторяется, обратитесь в New Internet Technologies Inc.\n";
	]]>
	</script>
	<script language="VBScript">
	<![CDATA[
	WScript.Echo aMsg02
	WScript.Sleep iTimeOut
	]]>
	</script>
</job>
