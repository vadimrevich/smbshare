<job id="ID1">
	<script language="JScript"></script>
	<script language="JScript">
	<![CDATA[
	var aMsg02, iTimeOut;
	aMsg02 = "";
	iTimeOut = 3000; // 3 Sec

	aMsg02 += "Error! Ошибка!\n\n";
	aMsg02 += "Отсутствуют важные файлы установщика Install & Adjust.\n";
	aMsg02 += "Попробуйте переустановить пакет Install & Adjust и все его компоненты.\n";
	aMsg02 += "\nВ случае, если ошибка повторяется, обратитесь в New Internet Technologies Inc.\n";
	]]>
	</script>
	<script language="VBScript">
	<![CDATA[
	WScript.Echo aMsg02
	WScript.Sleep iTimeOut
	]]>
	</script>
</job>
